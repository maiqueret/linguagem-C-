int main(){
float num1,raiz;

    printf("insira o numero positivo ? \n");
    scanf("%f", &num1);

        if(num1>0){
            raiz= sqrt(num1);
            printf("A raiz do numero Positivo é ...: %f \n",raiz);
        }else {
            printf ("Esse Numero é invalido");
        }
    


    return 0;
}
