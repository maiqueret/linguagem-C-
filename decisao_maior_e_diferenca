#include<stdio.h>
#include<stdlib.h>
#include <locale.h>

int main(){
int num1,num2, calc;


    printf("insira dois numeros...: \n");
    scanf("%d %d", &num1, &num2);

        if(num1>num2){
            
            printf("o maior numero e...:  %d\n",num1);
            calc= num1 - num2;
            printf("a diferenca entre eles e %d",calc);
            
        }else{
            printf(" o maior numero e...:  %d\n", num2);
            calc = num2 - num1;
            printf("a diferenca entre eles e ...: %d\n",calc);
        }
return 0;
}
